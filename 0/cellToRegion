FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    periodic_0
    {
        type            cyclic;
    }
    periodic_1
    {
        type            cyclic;
    }
    wall_2
    {
        type            zeroGradient;
    }
    wall_3
    {
        type            zeroGradient;
    }

}


